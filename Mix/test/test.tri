let 
   var i: Integer; 
   var j: Integer; 
   var k: Integer; 
   var num: Integer 
in 
  getint (var num); ! Number of iterations. 
  i:= 1; 
  j:=2;
  skip;
  putint(i); 
  put(' '); 
  k:= i+j; 
  i:= j; 
  num:= num - 1; 
  loop
   until j>3 
   do
    putint(j);
    j:=j+1
  repeat
end
let 
   var i: Integer; 
   var j: Integer; 
   var k: Integer;
   var w: Integer;
   var num: Integer 
in 
  getint (var num); ! Number of iterations. 
  i:= 1; 
  j:=2;
  skip;
  putint(i); 
  put(' '); 
  k:= i+j;  
  num:= num - 1; 
  loop for hola ~ w<5 to w=5 do skip repeat
end